package exam2025_03_26;

class static {
    public int a = 20;
    public int b = 0;
}

public class Test {

	public static void main(String[] args) {
        int a = 10;
        Static.b = a;
        Static st = new Static();

        System.out.println(st.b++);
        System.out.println(st.b);
        System.out.println(a);
        System.out.println(st.a);
    }

}
